<html>
	<head></head>
<body>
	<?php
		$matricula=$_POST['matricula'];
	        $senha=$_POST['senha'];
		$nome=$_POST['nome'];
		$sobrenome=$_POST['sobrenome'];
		$email=$_POST['email'];
		$conexao = mysql_connect("localhost","root","");
           	mysql_select_db("e2p");
		$sql_string="INSERT INTO usuarios VALUES("."'".$matricula."','".$senha."','".$nome."','".$sobrenome."','".$email."')";
		$resultado =  mysql_query($sql_string) or die(mysql_error());
        if($resultado)
        {
		    echo("<script>alert('Voce esta logado!');</script>");
            	    header("location: http://localhost/E2P/www/index.html");
		    mysql_close($conexao);
        }
            else
                mysql_close($conexao);
	?>
</body>
